# Theo-AI Environment Variables

# Model API Keys
# You only need one of these, but can provide both for flexibility
FORMATION_API_KEY=your_formation_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Model Provider Settings
# Options: 'formation' (default), 'openai', or other supported providers
DEFAULT_MODEL_PROVIDER=formation
# For Formation: 'best-quality', 'best-reasoning', 'best-speed', or specific model IDs
# For OpenAI: 'gpt-4o', 'gpt-4-turbo', etc.
DEFAULT_MODEL_ID=best-quality

# Formation API Settings (only needed if using Formation models)
FORMATION_API_BASE_URL=https://agents.formation.cloud/v1

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Google Calendar API credentials
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALENDAR_REDIRECT_URI=your_redirect_uri_here
GOOGLE_CALENDAR_CREDENTIALS_PATH=credentials.json

# API Authentication
API_SECRET_KEY=generate_a_secure_random_key_here

# API Endpoint (for Theo-AI API server)
# For local development, use http://localhost:8000
# For production, use https://agents.formation.cloud/<agent-id>/<version>
API_ENDPOINT=http://localhost:8000

# Optional third-party API keys
CRUNCHBASE_API_KEY=your_crunchbase_api_key_here

# Deployment Settings
ENVIRONMENT=development 